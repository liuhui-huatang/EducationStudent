<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@color/white"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_margin="@dimen/common_10"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="第一学期"
                android:textColor="@color/main_textone_color"
                android:textSize="@dimen/textSize_16" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="(满分10分)"
                android:textColor="@color/oqe_smalltext_color"
                android:layout_marginLeft="@dimen/common_5"
                android:textSize="@dimen/textSize_12" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/login_line_bg"
            />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/common_120"
            >
            <RelativeLayout
                android:id="@+id/oqemorality_lv_item_count"
                android:layout_width="@dimen/common_100"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_centerVertical="true"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    >
                    <RelativeLayout
                        android:layout_width="@dimen/common_100"
                        android:layout_height="@dimen/common_80"
                        android:orientation="vertical"
                        android:layout_centerVertical="true"
                        >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="19"
                    android:textColor="@color/oqe_bigcount_color"
                    android:layout_centerInParent="true"
                    android:textSize="@dimen/textSize_50" />
                <TextView
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/common_10"
                    android:layout_marginRight="@dimen/common_10"
                    android:text="分"
                    android:textColor="@color/black2"
                    android:textSize="@dimen/textSize_12" />
                    </RelativeLayout>
                    <RatingBar
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        style="@style/roomRatingBartwo"
                        android:numStars="5"
                        android:layout_gravity="center_horizontal"
                        android:rating="5"
                        />
                </LinearLayout>
            </RelativeLayout>
            <LinearLayout
                android:layout_toRightOf="@id/oqemorality_lv_item_count"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:orientation="vertical"
                >
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    >
                    <TextView
                        android:id="@+id/cxpd_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="身高：164cm"
                        android:textColor="@color/oqe_smalltext_color"
                        android:padding="@dimen/common_5"
                        android:layout_centerVertical="true"
                        android:textSize="@dimen/textSize_12" />
                    <TextView
                        android:id="@+id/oqemorality_lv_item_cxpdcount_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="2分"
                        android:layout_alignParentRight="true"
                        android:textColor="@color/oqe_smalltext_color"
                        android:padding="@dimen/common_5"
                        android:layout_centerVertical="true"
                        android:textSize="@dimen/textSize_12" />
                    <ProgressBar
                        android:layout_toLeftOf="@id/oqemorality_lv_item_cxpdcount_tv"
                        android:layout_width="@dimen/common_100"
                        android:layout_height="15dp"
                        android:layout_margin="@dimen/common_5"
                        android:layout_centerVertical="true"
                        android:max="5"
                        android:progress="2"
                        android:secondaryProgress="0"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:progressDrawable="@drawable/progerssbar_layer"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    >
                    <TextView
                        android:id="@+id/sspy_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="体重：55kg"
                        android:textColor="@color/oqe_smalltext_color"
                        android:padding="@dimen/common_5"
                        android:layout_centerVertical="true"
                        android:textSize="@dimen/textSize_12" />
                    <TextView
                        android:id="@+id/oqemorality_lv_item_sspycount_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="4分"
                        android:layout_alignParentRight="true"
                        android:textColor="@color/oqe_smalltext_color"
                        android:padding="@dimen/common_5"
                        android:layout_centerVertical="true"
                        android:textSize="@dimen/textSize_12" />
                    <ProgressBar
                        android:layout_toLeftOf="@id/oqemorality_lv_item_sspycount_tv"
                        android:layout_width="@dimen/common_100"
                        android:layout_height="15dp"
                        android:layout_margin="@dimen/common_5"
                        android:layout_centerVertical="true"
                        android:max="5"
                        android:progress="4"
                        android:secondaryProgress="0"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:progressDrawable="@drawable/progerssbar_layer"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    >
                    <TextView
                        android:id="@+id/ssjsxqypy_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="视力：5.0-4.0"
                        android:textColor="@color/oqe_smalltext_color"
                        android:padding="@dimen/common_5"
                        android:layout_centerVertical="true"
                        android:textSize="@dimen/textSize_12" />
                    <TextView
                        android:id="@+id/oqemorality_lv_item_ssjsxqypycount_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="5分"
                        android:layout_alignParentRight="true"
                        android:textColor="@color/oqe_smalltext_color"
                        android:padding="@dimen/common_5"
                        android:layout_centerVertical="true"
                        android:textSize="@dimen/textSize_12" />
                    <ProgressBar
                        android:layout_toLeftOf="@id/oqemorality_lv_item_ssjsxqypycount_tv"
                        android:layout_width="@dimen/common_100"
                        android:layout_height="15dp"
                        android:layout_margin="@dimen/common_5"
                        android:layout_centerVertical="true"
                        android:max="5"
                        android:progress="5"
                        android:secondaryProgress="0"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:progressDrawable="@drawable/progerssbar_layer"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    >
                    <TextView
                        android:id="@+id/gmsyzhcs_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="肺活量：1000k"
                        android:textColor="@color/oqe_smalltext_color"
                        android:padding="@dimen/common_5"
                        android:layout_centerVertical="true"
                        android:textSize="@dimen/textSize_12" />
                    <TextView
                        android:id="@+id/oqemorality_lv_item_gmsyzhcscount_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="3分"
                        android:layout_alignParentRight="true"
                        android:textColor="@color/oqe_smalltext_color"
                        android:padding="@dimen/common_5"
                        android:layout_centerVertical="true"
                        android:textSize="@dimen/textSize_12" />
                    <ProgressBar
                        android:layout_toLeftOf="@id/oqemorality_lv_item_gmsyzhcscount_tv"
                        android:id="@+id/my_progress"
                        android:layout_width="@dimen/common_100"
                        android:layout_height="15dp"
                        android:layout_margin="@dimen/common_5"
                        android:layout_centerVertical="true"
                        android:max="5"
                        android:progress="3"
                        android:secondaryProgress="0"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:progressDrawable="@drawable/progerssbar_layer"/>
                </RelativeLayout>
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>